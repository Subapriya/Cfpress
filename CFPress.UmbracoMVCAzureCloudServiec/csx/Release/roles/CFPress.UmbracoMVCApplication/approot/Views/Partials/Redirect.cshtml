@model CFPress.UmbracoMVCApplication.Models.RegisterMemberViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
   
   @if(!HttpContext.Current.User.Identity.IsAuthenticated)
    {
    <h2>Sign In or Register to comment or rate comments</h2>
    <div>

        <table>
            <tr>
                <td>@Html.Hidden("ReturnUrl", Model.ReturnUrl)
                  </td>
                <td class="button">@Html.ActionLink("Register", "HandleSubmittoRegister", "Redirect")</td>
                <td rowspan="2"></td>
                <td><br /></td>
                <td class="button">@Html.ActionLink("Sign In", "HandleSubmittoLogin", "Redirect")</td>
            </tr>
        </table>
    </div>
    }
    
</body>
</html>
